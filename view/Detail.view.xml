<mvc:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" controllerName="Variantconf.view.Detail">
    <Page id="detailPage" navButtonPress="onNavBack" title="{i18n&gt;detailTitle}" showNavButton="{device&gt;/isPhone}">
        <content>
            <ObjectHeader id="detailHeader" title="{ATWTB}" number="{ATFLV}" numberUnit="{ATWRT}" introActive="false" titleActive="false" iconActive="false">
                <attributes id="detailAttributes">                
                    <ObjectAttribute id="attribute" text="{i18n&gt;detailText}" active="false">
                    </ObjectAttribute>
                </attributes>
                <firstStatus id="detailStatus">
                    <ObjectStatus id="status" text="{ATWTB}">
                    </ObjectStatus>
                </firstStatus>
            </ObjectHeader>
            <IconTabBar id="idIconTabBar" expanded="{device&gt;/isNoPhone}">
    			<items id="detailsItems">
                    <IconTabFilter id="iconTabFilter1" key="selfInfo" icon="sap-icon://hint" text="Programm">
                        <content>
							<f:SimpleForm id="iconTabFilter1form" minWidth="1024" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1">
								<f:content>
									<Label id="label1" text="Programm Name">
									</Label>
									<Text id="text1" text="{ATFLV}" maxLines="0">
									</Text>
									<Label id="label2" text="Beschreibung">
									</Label>
									<Text id="text2" text="{ATWRT}" maxLines="0">
									</Text>
									<Label id="label3" text="Lieferant">
									</Label>
									<Text id="text3" text="{ATWRT}" maxLines="0">
									</Text>
									<core:ExtensionPoint name="extIconTabFilterForm1"/>
								</f:content>
							</f:SimpleForm>
                        </content>
                         <content>
                                <l:Grid defaultSpan="L12 M12 S12" width="auto">
                        		<l:content>
                        			<f:SimpleForm id="SimpleFormDisplay354" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" title="Technische Merkmale" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
                        				<f:content>
                        					<Label text="Unit of Measure"/>
                        					<Text text="{SupplierName}"/>
                        					<Label text="Sitz"/>
                        					<Text text="{Street} {HouseNumber}"/>
                        					<Label text="Rücken"/>
                        					<Text text="{ZIPCode} {City}"/>
                        					<Label text="Armlehnen"/>
                        					<Text text="{Country}"/>
                        					<Label text="Füße"/>
                        					<Text text="{Country}"/>
                        				</f:content>
                        			</f:SimpleForm>
                        		</l:content>
                        	</l:Grid>
                            </content>
                    </IconTabFilter>
                    <IconTabFilter id="iconTabFilter2" text="Type" icon="sap-icon://group">
                          <content>
						     <List id="Type" items="{/ZUI5_TEST_TVALUES_SET?$filter=ATNAM eq 'FIORI_TYP'}" enabled="true">
                                    <items>
                                        <StandardListItem type="Navigation" counter="0" title="{ATWTB}" description="Comfort for one" icon="data:image/png;base64,"/>
                                        </items>
                                </List>
                            </content>
                    </IconTabFilter>
                    <IconTabFilter text="Farbe" icon="sap-icon://palette" id="__filter0">
                        <content>
                            <Select id="Farbe" items="{/ZUI5_TEST_TVALUES_SET?$filter=ATNAM eq 'FIORI_FARBE'}" enabled="true">
                                    <core:ListItem text="{ATWTB}"/>
                            </Select>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter text="Bezügstyp" icon="sap-icon://puzzle" id="__filter1">
                            <content>
                                <Label text="Wählen Sie bitte die gewünschte Bezugart" width="100%"/>
                                 	<RadioButtonGroup xmlns="sap.m"
                                 		id="Stoff"
                                 		buttons="{/ZUI5_TEST_TVALUES_SET?$filter=ATNAM eq 'FIORI_BEZUGSSTOFF'}"
                                 		width="500px"
                                 		columns="1"
                                 		selectedIndex="0"
                                 		enabled="true"
                                 		class="sapUiMediumMarginBottom"
                                 		select="">
                                 			<tooltip></tooltip> <!-- sap.ui.core.TooltipBase -->
                                 			<dependents></dependents> <!-- sap.ui.core.Control, since 1.19 -->
                                 			<buttons></buttons><!-- sap.m.RadioButton -->
                                     	    <RadioButton text="{ATWTB}"/>
                                 	</RadioButtonGroup> 
                    			</content>
                    </IconTabFilter>
                    <IconTabFilter text="Kissen" icon="sap-icon://family-care" id="__filter2">
                        <content>
                            <Label text="Select your choice" width="100%" id="__label5"/>
                            <Slider id="__slider0"/>
                        </content>
                    </IconTabFilter>
    				<core:ExtensionPoint name="extIconTabFilter"/>
                </items>
            </IconTabBar>
    		<core:ExtensionPoint name="extDetail"/>
        </content>
        <footer id="detailFooter">
            <Toolbar id="detailToolbar">
                <content>
                    <ToolbarSpacer id="toolbarSpacer">
                    </ToolbarSpacer>
                    <Button id="actionButton" press="openActionSheet" icon="sap-icon://action">
                    </Button>
                </content>
            </Toolbar>
        </footer>
    </Page>
</mvc:View>