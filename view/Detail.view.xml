<mvc:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" controllerName="Variantconf.view.Detail">
    <Page id="detailPage" navButtonPress="onNavBack" title="{i18n&gt;detailTitle}" showNavButton="{device&gt;/isPhone}">
        <content>
            <ObjectHeader id="detailHeader" title="{ATWTB}" number="" numberUnit="" introActive="false" titleActive="false" iconActive="false">
                <attributes id="detailAttributes">                
                    <ObjectAttribute id="attribute" text="{i18n&gt;detailText}" active="false">
                    </ObjectAttribute>
                </attributes>
                <firstStatus id="detailStatus">
                    <ObjectStatus id="status" text="{ATWTB}">
                    </ObjectStatus>
                </firstStatus>
            </ObjectHeader>
            <IconTabBar expanded="{device&gt;/isNoPhone}" select="onSelectChanged" id="idIconTabBar">
    			<items id="detailsItems">
                   <IconTabFilter id="iconTabFilter1" key="selfInfo" icon="sap-icon://hint" text="Programm">
                        <content>
							<f:SimpleForm id="Prog" minWidth="1024" editable="false" layout="ResponsiveGridLayout" title="Technische Merkmale" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" items="{/ZUI5_TEST_TVALUETEXTE_SET}">
								<f:content>
									<Label id="label1" text="Programm ID">
									</Label>
									<Text id="text1" text="{ATWRT}" maxLines="0">
									</Text>
									<Label id="label2" text="Beschreibung">
									</Label>
									<Text id="text2" text="{TDLINE}" maxLines="0">
									</Text>
									<Label id="label3" text="Beschreibung">
									</Label>
                                    <Text id="text3" text="{ATWTB}" maxLines="0">
                                    </Text>
                                    <Label id="label4" text="Lieferant">
                                    </Label>
                                    <Text id="text4" text="Supplier_hardKodiert" maxLines="0">
                                    </Text>
                                    <Label id="label5" text="Bewertung">
                                    </Label>
                                    <RatingIndicator id="Rating1" maxValue="5" class="sapUiSmallMarginBottom" value="4" tooltip="Rating Tooltip" enabled="true" iconSize="0.8rem" verticalAdjustment="-15" verticalAlignContent="true"/>
									<core:ExtensionPoint name="extIconTabFilterForm1"/>
								</f:content>
							</f:SimpleForm>
                            </content>
                    </IconTabFilter> 
                    <IconTabFilter id="iconTabFilter2" key="2" text="Typ" icon="sap-icon://group">
					     <content>
			             <Text text="W채hlen Sie eine Option aus"/>     
                            <l:Grid
                        		class="sapUiSmallMarginTop"
                        		hSpacing="2"
                        		defaultSpan="L8 M8 S12">
                        		<l:content>
                            		<Image width="140px" height="140px" id="odata_img01">
                        				<layoutData>
                        					<l:GridData
                        						span="L4 M4 S10"
                        						linebreakL="true"
                        						linebreakM="true"
                        						linebreakS="true"/>
                        				</layoutData>
                        			</Image>
                        				<VBox>
                        				    <RadioButtonGroup xmlns="sap.m"
                                         		id="Typ"
                                         		buttons="{/ZUI5_TEST_TVALUES_SET}"
                                         		width="500px"
                                         		columns="1"
                                         		selectedIndex="0"
                                         		enabled="true"
                                         		class="sapUiMediumMarginBottom"
                                         		select="radiobuttonselect_T">
                                              <buttons>
                                                    <RadioButton id="RBG1" text="{ATWRT}"/>              
                                               </buttons>
                                        	</RadioButtonGroup>
                            			</VBox>
                        		</l:content>
                    	   </l:Grid> 
                        </content>
                    </IconTabFilter>
                    <IconTabFilter text="Farbe" key="3" icon="sap-icon://palette" id="iconTabFilter3">
                        <content>
                            <l:Grid
                        		class="sapUiSmallMarginTop"
                        		hSpacing="2"
                        		defaultSpan="L8 M8 S12">
                        		<l:content>
                            		<Image src="/sap/opu/odata/sap/ZUI5_TEST_TVC_SRV/ZUI5_TEST_TVALUES_SET(ATNAM='FIORI_JPEG',ATWRT='BA_SE_GR_KL')/$value" width="140px" height="140px" id="odata_img2">
                        				<layoutData>
                        					<l:GridData
                        						span="L4 M4 S10"
                        						linebreakL="true"
                        						linebreakM="true"
                        						linebreakS="true"/>
                        				</layoutData>
                        			</Image>
                        				<VBox>
                        				    <ComboBox xmlns="sap.m" id="Farbe" items="{/ZUI5_TEST_TVALUES_SET}" selectionChange="onChange_F">
            						      	<items>
            				                <core:Item key="{ATWRT}" text="{ATWTB}"/>
            				                </items>
            						      </ComboBox>
                            			</VBox>
                        		</l:content>
                    	   </l:Grid>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter text="Bez체gstyp" key="4" icon="sap-icon://puzzle" id="iconTabFilter4">
                            <content>
                            <Label text="W채hlen Sie bitte die gew체nschte Bezugart" width="100%"/>
                                 	<RadioButtonGroup xmlns="sap.m"
                                 		id="Stoff"
                                 		buttons="{/ZUI5_TEST_TVALUES_SET}"
                                 		width="500px"
                                 		columns="1"
                                 		selectedIndex="0"
                                 		enabled="true"
                                 		class="sapUiMediumMarginBottom"
                                 		select="radiobuttonselect_B">
                                              <buttons>
                                                    <RadioButton id="RBG2" text="{ATWRT}"/>              
                                               </buttons>
                                 	</RadioButtonGroup> 
                    			</content>
                     </IconTabFilter>
                    <IconTabFilter text="Kissen" key="5" icon="sap-icon://family-care" id="iconTabFilter5">
                        <content>
                            <ComboBox xmlns="sap.m" id="Kissen" items="{/ZUI5_TEST_TVALUES_SET}" selectionChange="onChange_K">
						      	<items>
				                <core:Item key="{ATWRT}" text="{ATWRT}"/>
				                </items>
						      </ComboBox>
                        </content> 
                    </IconTabFilter> 
    				<core:ExtensionPoint name="extIconTabFilter"/>
                </items>
            </IconTabBar>
    		<core:ExtensionPoint name="extDetail"/>
        </content>
        <footer id="detailFooter">
            <Toolbar id="detailToolbar">
                <content>
                    <ToolbarSpacer id="toolbarSpacer">
                    </ToolbarSpacer>
                    <Button id="actionButton" press="openActionSheet" icon="sap-icon://action">
                    </Button>
                </content>
            </Toolbar>
        </footer>
    </Page>
</mvc:View>